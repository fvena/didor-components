{
  "name": "didor-components",
  "version": "0.1.0",
  "description": "Libreria UI de componentes con Vue para Didor",
  "author": "Francisco Vena <fvena32@gmail.com> (http://www.fvena.com/)",
  "homepage": "https://github.com/fvena/didor-components#readme",
  "private": false,
  "license": "MIT",
  "main": "./dist/az.common.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "docs": "didor serve",
    "build": "vue-cli-service build --target lib --name az ./packages/index.js",
    "test": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:stylelint": "stylelint --fix",
    "svg": "vsvg -s ./src/assets/icons/svg -t ./src/assets/icons/sprite",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.1.2",
    "deepmerge": "^4.2.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^10.2.7",
    "node-sass": "^4.12.0",
    "prettier": "^2.0.5",
    "sass-loader": "^8.0.2",
    "standard-version": "^8.0.0",
    "stylelint": "^13.5.0",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.17.2",
    "vue-cli-plugin-pug": "~1.0.7",
    "vue-template-compiler": "^2.6.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  }
}
